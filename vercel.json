{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/",
      "dest": "/public/index.html",
      "headers": {
        "Cache-Control": "s-maxage=0"
      }
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ]
}
